{
  "manifest_version": 3,
  "name": "Active tab right",
  "version": "1.0",
  "description": "Automatically moves the active tab to the rightmost position.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
